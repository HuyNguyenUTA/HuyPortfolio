{
  "name": "postcss-place",
  "description": "Use a place-* shorthand for align-* and justify-* in CSS",
  "license": "CC0-1.0",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "homepage": "https://github.com/jonathantneal/postcss-place#readme",
  "repository": "jonathantneal/postcss-place",
  "bugs": "https://github.com/jonathantneal/postcss-place/issues",
  "version": "4.0.1",
  "main": "index.cjs.js",
  "module": "index.es.mjs",
  "files": [
    "index.cjs.js.map",
    "index.es.mjs",
    "index.es.mjs.map"
  ],
  "scripts": {
    "prepublishOnly": "npm test",
    "pretest": "rollup -c .rollup.js --silent",
    "test": "echo 'Running tests...'; npm run test:js && npm run test:tape",
    "test:ec": "echint --ignore index.*.js test",
    "test:js": "eslint *.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "postcss-tape"
  },
  "dependencies": {
    "postcss": "^7.0.2",
    "postcss-values-parser": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-config-dev": "^2.0.0",
    "postcss-tape": "^2.2.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^4.0.3"
  },
  "keywords": [
    "alignments",
    "aligns",
    "contents",
    "css",
    "justifies",
    "justify",
    "postcss",
    "postcss-plugin",
    "selfs",
    "shorthands"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  }
}
