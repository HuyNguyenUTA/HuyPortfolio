{
  "name": "shallowequal",
  "description": "Like lodash isEqualWith but for shallow equal.",
  "license": "MIT",
  "author": "Alberto Leal <mailforalberto@gmail.com> (github.com/dashed)",
  "repository": "dashed/shallowequal",
  "version": "1.1.0",
  "main": "index.js",
  "files": [
    "index.js.flow",
    "index.original.js"
  ],
  "scripts": {
    "build": "npm run build:strip-flow && npm run build:gen-flow",
    "build:gen-flow": "flow gen-flow-files index.original.js > index.js.flow",
    "build:strip-flow": "flow-remove-types --pretty index.original.js > index.js",
    "precommit": "lint-staged",
    "lint": "eslint index.js test",
    "prepublish": "npm run build && npm run pretty && npm run lint && npm run test",
    "test": "mocha --require babel-register",
    "travis": "npm run lint && npm run test",
    "pretty": "prettier --write --tab-width 2 'test/**/*.js' '*.{js,js.flow}'"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "eslint": "^4.7.1",
    "flow-bin": "^0.75.0",
    "flow-remove-types": "^1.2.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^5.0.0",
    "prettier": "^1.9.2"
  },
  "keywords": [
    "compare",
    "equal",
    "isequal",
    "isequalwith",
    "shallow",
    "shallowequal"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,js.flow}": [
      "prettier --write",
      "git add"
    ]
  }
}
