    "test": "node --throw-deprecation --track-heap-objects test.js"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^5.0.0",
    "eslint": "^4.16.0",
    "istanbul": "^0.4.5",
    "tape": "^4.8.0"
  },
  "keywords": [
    "check",
    "file",
    "module",
    "path",
    "resolvable",
    "resolve"
  ],
  "eslintConfig": {
    "extends": "@shinnn/node",
    "rules": {
      "no-var": "off",
      "prefer-template": "off"
    }
  }
}
