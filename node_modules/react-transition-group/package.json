{
  "name": "react-transition-group",
  "description": "A react component toolset for managing animations",
  "license": "BSD-3-Clause",
  "author": "",
  "homepage": "https://github.com/reactjs/react-transition-group#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-transition-group.git"
  },
  "bugs": {
    "url": "https://github.com/reactjs/react-transition-group/issues"
  },
  "version": "4.3.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "dom-helpers": "^5.0.1",
    "loose-envify": "^1.4.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": ">=16.6.0",
    "react-dom": ">=16.6.0"
  },
  "keywords": [
    "addons",
    "animation",
    "css",
    "react",
    "transition",
    "transition-group",
    "transitions"
  ],
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "testRegex": "-test\\.js",
    "setupFiles": [
      "./test/setup.js"
    ],
    "roots": [
      "<rootDir>/test"
    ]
  },
  "release": {
    "pkgRoot": "lib",
    "verifyConditions": [
      "@semantic-release/changelog",
      "semantic-release-alt-publish-dir",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "semantic-release-alt-publish-dir",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
