{
  "name": "map-age-cleaner",
  "description": "Automatically cleanup expired items in a Map",
  "license": "MIT",
  "author": "Sam Verschueren <sam.verschueren@gmail.com> (github.com/SamVerschueren)",
  "repository": "SamVerschueren/map-age-cleaner",
  "version": "0.1.3",
  "main": "dist/index.js",
  "files": [
    "dist/index.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "del-cli dist",
    "lint": "tslint --format stylish --project .",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build -- --sourceMap",
    "test": "npm run lint && nyc ava dist/test.js"
  },
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "dependencies": {
    "p-defer": "^1.0.0"
  },
  "devDependencies": {
    "@types/delay": "^2.0.1",
    "@types/node": "^10.7.1",
    "ava": "^0.25.0",
    "codecov": "^3.0.0",
    "del-cli": "^1.1.0",
    "delay": "^3.0.0",
    "nyc": "^12.0.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.9.0",
    "typescript": "^3.0.1"
  },
  "keywords": [
    "age",
    "cleaner",
    "expiration",
    "expire",
    "expiring",
    "map",
    "maxage"
  ],
  "engines": {
    "node": ">=6"
  },
  "nyc": {
    "exclude": [
      "dist/test.js"
    ]
  }
}
