{
  "autodiscover": false,
  "commitMessagePrefix": "[dist]",
  "ignoreNpmrcFile": true,
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "minor": {
        "groupName": "all non-major dependencies",
        "groupSlug": "all-minor-patch"
      }
    }
  ],
  "platform": "github",
  "rangeStrategy": "replace",
  "requireConfig": true
}
